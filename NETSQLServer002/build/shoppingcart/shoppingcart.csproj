<Project Sdk="Microsoft.NET.Sdk" ToolsVersion="Current">
	
	
	<PropertyGroup>
		<AssemblyName>shoppingcart</AssemblyName>
		<TargetFramework>$(TargetFramework)</TargetFramework>
		<OutputType>Library</OutputType>
		<GenerateDependencyFile>false</GenerateDependencyFile>
		<CopyLocalLockFileAssemblies>false</CopyLocalLockFileAssemblies>
		<PostgresqlSupport>false</PostgresqlSupport>
		<OfficeSupport>false</OfficeSupport>
		<MailSupport>false</MailSupport>
		<GrpcClientSupport>false</GrpcClientSupport>
		<PdfLibrary>itext</PdfLibrary>
	</PropertyGroup>
	
	<ItemGroup>
		<ProjectReference Include=".\..\GeneXus.Programs.Common\GeneXus.Programs.Common.csproj" >
			<OutputItemType>$(CommonOutputItemType)</OutputItemType>
		</ProjectReference>

	</ItemGroup>

	<ItemGroup>
		<Reference Include="GeneXus.dll" > 
			<HintPath>$(WebBinDirectory)\GeneXus.dll</HintPath>
		</Reference>
		<Reference Include="GeneXusUnanimo.dll" > 
			<HintPath>$(WebBinDirectory)\GeneXusUnanimo.dll</HintPath>
		</Reference>
		<Reference Include="GeneXus.dll" > 
			<HintPath>$(WebBinDirectory)\GeneXus.dll</HintPath>
		</Reference>
		<Reference Include="GeneXusUnanimo.dll" > 
			<HintPath>$(WebBinDirectory)\GeneXusUnanimo.dll</HintPath>
		</Reference>


	</ItemGroup>

	<ItemGroup>
		<Compile Include = "$(WebDirectory)\shoppingcartreport.cs" />
		<Compile Include = "$(WebDirectory)\accumulatepoints.cs" />
		<Compile Include = "$(WebDirectory)\shoppingcart.cs" />
		<Compile Include = "$(WebDirectory)\general\security\isauthorized.cs" />
		<Compile Include = "$(WebDirectory)\pointcard_bc.cs" />
		<Compile Include = "$(WebDirectory)\shoppingcart_bc.cs" />

	</ItemGroup>
	<ItemGroup Condition="'$(PostgresqlSupport)'=='true'">
		<PackageReference Include="Npgsql" Version="8.0.3" PrivateAssets="All"/>
	</ItemGroup>

	<Import Project="..\DotNetCoreBaseProject.targets"/>	
</Project>

